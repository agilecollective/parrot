alias /xhprof "/usr/share/php/xhprof_html/"

<Directory /usr/share/php/xhprof_html/>
    Options Indexes FollowSymLinks MultiViews
    AllowOverride All
    Order allow,deny
    allow from all
    DirectoryIndex index.php

    <FilesMatch "\.php$">
      SetHandler  "proxy:fcgi://127.0.0.1:9999"
    </FilesMatch>
</Directory>
